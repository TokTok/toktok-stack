genrule(
    name = "regen_config",
    srcs = [
        "configure",
        "libavcodec/allcodecs.c",
        "libavcodec/bitstream_filters.c",
        "libavdevice/alldevices.c",
        "libavfilter/allfilters.c",
        "libavformat/allformats.c",
        "libavformat/protocols.c",
    ],
    outs = [
        "config.asm",
        "config.h",
        "libavcodec/bsf_list.c",
        "libavformat/protocol_list.c",
        "libavutil/avconfig.h",
    ],
    cmd = """
        export PATH=$$PWD/$$(dirname $(location @yasm)):$$PATH
        cd `dirname $(location configure)`
        ./configure --disable-v4l2_m2m
        DIR=`pwd`
        cd -
        for i in $(OUTS); do
          cp $$DIR/`echo $$i | sed -e 's|$(GENDIR)/external/ffmpeg/||'` $$i
        done
    """,
    tools = ["@yasm"],
)

genrule(
    name = "regen_ffversion",
    srcs = [
        "ffbuild/version.sh",
        "VERSION",
    ],
    outs = ["libavutil/ffversion.h"],
    cmd = "$(location ffbuild/version.sh) external/ffmpeg $@",
)

