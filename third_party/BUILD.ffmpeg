    "-DHAVE_AV_CONFIG_H",
    "-Iexternal/toktok/third_party/ffmpeg",
] + select({
    "@toktok//tools/config:freebsd": ["-Iexternal/toktok/third_party/ffmpeg/freebsd"],
    "@toktok//tools/config:linux": [
        "-D_ISOC99_SOURCE",
        "-D_POSIX_C_SOURCE=200112",
